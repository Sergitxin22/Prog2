package poo;

public class Circulo {
	
	private double radio;
	private String color;

	public Circulo() {
		this.radio = 5;
		this.color = "rojo";
	}

	// Getters
	public double getRadio() {
		return radio;
	}
	
	public String getColor() {
		return color;
	}

	// Setters
	public void setRadio(double radio) {
		this.radio = radio;
	}
	
	public void setColor(String color) {
		this.color = color;
	}
}
